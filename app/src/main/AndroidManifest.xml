<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fae.edu.sqlitetestgit" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NoticiaListActivity"
            android:label="@string/title_activity_noticia_list" >
        </activity>
        <activity
            android:name=".NoticiaFormActivity"
            android:label="@string/title_activity_noticia_form" >
        </activity>
        <activity
            android:name=".NoticiaDetailActivity"
            android:label="@string/title_activity_noticia_detail" >
        </activity>
    </application>

</manifest>
